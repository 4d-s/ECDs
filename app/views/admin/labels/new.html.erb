<div class = "col-xs-9">
	<h1>レーベル登録</h1>
		<%= form_for(@label, url: admin_labels_path) do |f| %>
		<br>
		<div class="row">
			<br>
			<div class="col-xs-6">
			<label>レーベル名</label>
				<%= f.text_field :label_name, class:'form-control', :placeholder => "レーベル名を入力"  %>
			</div>
		</div>
		<br>
		<br>
		<%= f.submit "登録", class: "btn btn-primary" %>
		<% end %>
</div>