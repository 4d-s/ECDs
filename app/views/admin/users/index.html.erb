<div class = "admin-users-area col-xs-9">
	<div class = "admin-users-area">
	<h1 class= "admin-users-title">ユーザー一覧</h1>

	<table class ="table table-hover table-inverse">
		<thead>
			<tr>
				<th>ID</th>
				<th>ユーザー名</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td><%= user.id %></td>
					<td><%= link_to user.name, admin_user_path(user.id) %></td>
				</tr>
				<% end %>
		</tbody>
	</table>
	</div>
</div>